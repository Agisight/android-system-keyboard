<merge
    xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin"
>
    <!-- U+04E9: "ө" CYRILLIC SMALL LETTER BARRED O (CYRILLIC SMALL LETTER EF - "ф" - moved to longpress) -->
    <Key
        latin:keySpec="&#x04E9;"
        latin:keyHintLabel="ф"
        latin:moreKeys="&#x0444;" />
    <Key
        latin:keySpec="&#x044B;"
        latin:moreKeys="!text/morekeys_east_slavic_row2_2" />
    <!-- U+0432: "в" CYRILLIC SMALL LETTER VE -->
    <Key
        latin:keySpec="&#x0432;" />
    <!-- U+0430: "а" CYRILLIC SMALL LETTER A -->
    <Key
        latin:keySpec="&#x0430;"
        latin:moreKeys="!text/morekeys_cyrillic_a" />
    <!-- U+043F: "п" CYRILLIC SMALL LETTER PE -->
    <Key
        latin:keySpec="&#x043F;" />
    <!-- U+0440: "р" CYRILLIC SMALL LETTER ER -->
    <Key
        latin:keySpec="&#x0440;" />
    <!-- U+043E: "о" CYRILLIC SMALL LETTER O -->
    <Key
        latin:keySpec="&#x043E;"
        latin:moreKeys="!text/morekeys_cyrillic_o" />
    <!-- U+043B: "л" CYRILLIC SMALL LETTER EL -->
    <Key
        latin:keySpec="&#x043B;" />
    <!-- U+0434: "д" CYRILLIC SMALL LETTER DE (HYPHEN-MINUS `-`) -->
    <Key
        latin:keySpec="&#x0434;"
        latin:keyHintLabel="-"
        latin:moreKeys="&#x002D;" />
    <!-- U+0436: "ж" CYRILLIC SMALL LETTER ZHE -->
    <Key
        latin:keySpec="&#x0436;" />
    <!-- U+044D: "э" CYRILLIC SMALL LETTER E -->
    <Key
        latin:keySpec="&#x044D;"
        latin:moreKeys="!text/morekeys_east_slavic_row2_11" />
</merge>
